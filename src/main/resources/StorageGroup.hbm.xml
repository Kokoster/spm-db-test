<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="spm.db.StorageGroup" table="dwd_storagegroup">
        <id name="id" type="int" column="storagegroupkey"/>
        <property name="name" column="storagegroupid" type="string"/>
    </class>
</hibernate-mapping>